{
  "manifest_version": 2,

  "name": "William Chops",
  "description": "This extension will remove profanity from pages",
  "version": "1.0",

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery-1.11.1.min.js", "replacer.js"],
      "run_at:": "document_start",
      "all_frames": true
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}