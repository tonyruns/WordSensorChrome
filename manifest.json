{
  "manifest_version": 2,

  "name": "One-click Kittens",
  "description": "This extension will remove profanity from pages",
  "version": "1.0",

  "content_scripts": [
    {
      "matches": ["http://*/*"],
      "js": ["replacer.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}