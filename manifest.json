{
  "manifest_version": 2,

  "name": "William Chops",
  "description": "This extension will remove profanity from pages",
  "version": "1.0",

  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["replacer.js"],
      "run_at:": "document_end"
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}