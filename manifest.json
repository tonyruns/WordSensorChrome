{
  "manifest_version": 2,

  "name": "One-click Kittens",
  "description": "This extension will remove profanity from pages",
  "version": "1.0",

  "permissions": [
    "https://secure.flickr.com/"
    "tabs", "http://*/*"
  ],
  "content_scripts": [
    {
      "matches": ["http://*/*"],
      "js": ["replacer.js"],
      "run_at": "document_end"
    }
  ]
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}